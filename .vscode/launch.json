{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Baseline Experiment",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "-c",
                "config/baseline.yaml"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {},
            "python": "${workspaceFolder}/.venv/bin/python"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}